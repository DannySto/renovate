{
  "extends": [
    "config:recommended"
  ],
  "enabledManagers": [
    "custom.regex"
  ],
  "customManagers": [
    {
      "customType": "regex",
      "managerFilePatterns": [
        "/^Dockerfile$/"
      ],
      "matchStrings": [
        "FROM python:\\\"(?<version>[\\d.]+)\\\"-slim-bookworm@sha256:(?<currentValue>[a-f0-9]{64})"
      ],
      "datasourceTemplate": "docker",
      "depNameTemplate": "python",
      "currentValueTemplate": "{{currentValue}}"
    }
  ]
}
